---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>dustinchilson.com</title>
  <link href="https://dustinchilson.com/"/>
  <link type="application/atom+xml" rel="self" href="https://dustinchilson.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://dustinchilson.com</id>
  <author>
    <name>Dustin Chilson</name>
    <email>Email</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>https://dustinchilson.com{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="https://dustinchilson.com{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Dustin Chilson</name>
      <uri>https://dustinchilson.com</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>